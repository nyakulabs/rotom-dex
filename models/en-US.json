{
    "invocation":"rotom dex",
    "intents":[
        {
            "name":"HelloWorldIntent",
            "phrases":[
                "hello",
                "say hello",
                "say hello world"
            ]
        },
        {
            "name":"MyNameIsIntent",
            "phrases":[
                "{name}",
                "my name is {name}",
                "i am {name}",
                "you can call me {name}"
            ],
            "inputs":[
                {
                    "name":"name",
                    "type":{
                        "alexa":"AMAZON.US_FIRST_NAME",
                        "dialogflow":"@sys.given-name"
                    }
                }
            ]
        },
        {
            "name":"PokemonInfo",
            "phrases":[
                "tell me about {pokemon}",
                "{pokemon} info",
            ],
            "inputs":[
                {
                    "name":"pokemon",
                    "type":{
                        "alexa":"AMAZON.SearchQuery",
                        "dialogflow":"@sys.any"
                    }
                }
            ]
        }
    ]
}
